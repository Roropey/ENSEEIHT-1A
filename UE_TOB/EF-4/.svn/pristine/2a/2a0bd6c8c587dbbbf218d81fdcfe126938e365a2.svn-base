package Ressources;

import Utile.*;

public class Baie extends Ressource{

    /** Constructeur d'une baie dont la position est aléatoire dans un intervalle
     * @param min_x absicisse minimale de la baie
     * @param max_x absicisse maximale de la baie
     * @param min_y ordonnée minimale de la baie
     * @param max_y ordonnée maximale de la baie
     */
    public Baie(int min_x, int max_x, int min_y, int max_y){
        super(min_x, max_x, min_y, max_y,"Baie",2,Categorie.fruit);
    }

    /** Constructeur d'une baie personnalisable dont la position connue
     * @param x absicisse de la baie (>=0)
     * @param y ordonnée de la baie (>=0)
     * @param nom nom de la baie
     * @param nutrition valeur nutritionnaire de la baie
     * @param categorie catégorie de la baie
     */
    public Baie(int x, int y){
        super(x, y,"Baie",2,Categorie.fruit);
    }

}