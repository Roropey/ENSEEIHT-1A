package Ressources;

import Utile.*;

public class Algue extends Ressource{

    /** Constructeur d'une algue dont la position est aléatoire dans un intervalle
     * @param min_x absicisse minimale de l'algue
     * @param max_x absicisse maximale de l'algue
     * @param min_y ordonnée minimale de l'algue
     * @param max_y ordonnée maximale de l'algue
     */
    public Algue(int min_x, int max_x, int min_y, int max_y){
        super(min_x, max_x, min_y, max_y,"Algue",5,Categorie.plante);
    }

    /** Constructeur d'une algue personnalisable dont la position connue
     * @param x absicisse de l'algue (>=0)
     * @param y ordonnée de l'algue (>=0)
     * @param nom nom de l'algue
     * @param nutrition valeur nutritionnaire de l'algue
     * @param categorie catégorie de l'algue
     */
    public Algue(int x, int y){
        super(x, y,"Algue",5,Categorie.plante);
    }
}